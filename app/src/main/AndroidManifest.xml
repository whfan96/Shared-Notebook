<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lungteng.finalapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/pen"
        android:label="@string/app_name"
        android:roundIcon="@drawable/pen"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Signup" />
        <activity
            android:name=".PersonalNotes"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PublicNotes"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".DataPublicNotes"/>
        <activity android:name=".DataPersonalNotes"/>
        <activity android:name=".ModifyPersonal"/>
        <activity android:name=".ModifyPublic"/>
        <activity android:name=".EditPersonalNotes" />

    </application>

</manifest>